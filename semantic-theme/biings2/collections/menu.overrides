/*******************************
         Theme Overrides
*******************************/

.ui.secondary.pointing.menu .item
{
	letter-spacing: 0.6px;
	margin: 0em 1em -@secondaryPointingBorderWidth;
}
.ui.secondary.pointing.menu .item > .svg
{
	fill: @secondaryItemColor;
}
.ui.secondary.menu .active.item > .svg,
.ui.secondary.menu .item:hover > .svg {
	fill: @secondaryActiveItemColor;
}