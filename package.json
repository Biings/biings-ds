{
  "name": "biings-ds",
  "version": "0.0.1",
  "description": "Biings Design System",
  "main": "src/styles/bds.css",
  "scripts": {
    "sass": "sass -I node_modules src/styles/bulma-bds/bulma.sass src/styles/bulma-bds/bulma.css",
    "postcss": "postcss --config src/styles/postcss-config.json",
    "styles": "npm run sass && npm run postcss",
    "icons": "grunt",
    "build": "npm run styles && npm run icons"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ForsysInteractive/biings-ds.git"
  },
  "keywords": [
    "biings",
    "design system",
    "icons"
  ],
  "author": {
    "name": "Biings Technologies SA",
    "email": "email@biings.com",
    "url": "http://www.biings.com"
  },
  "contributors": [
    "Frédéric Andreae"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ForsysInteractive/biings-ds/issues"
  },
  "homepage": "https://github.com/ForsysInteractive/biings-ds#readme",
  "devDependencies": {
    "bulma": "^0.7.1",
    "grunt": "~0.4.1",
    "grunt-cli": "^1.2.0",
    "grunt-contrib-clean": "~0.5.0",
    "grunt-contrib-copy": "~0.5.0",
    "grunt-contrib-less": "~0.11.0",
    "grunt-contrib-requirejs": "~0.4.4",
    "grunt-svgmin": "~0.4.0",
    "grunt-svgstore": "^0.5.0",
    "postcss": "^6.0.22",
    "postcss-cli": "^5.0.0"
  }
