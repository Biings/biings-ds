{
    "name": "biings-ds",
    "version": "1.22.6",
    "description": "Biings Design System",
    "main": "build/bds.css",
    "scripts": {
        "sass": "\"node-sass\" --include-path node_modules src/styles/bulma/bulma.sass src/styles/bulma/bulma.css",
        "postcss": "postcss src/styles/bds.css --config src/styles/postcss.config.js -o build/bds.css",
        "copytodoc": "cp build/bds.css docs/bds.css",
        "copycomb": "cp src/styles/.csscomb.json build/",
        "icons": "grunt",
        "styles": "npm run sass && npm run postcss && npm run copytodoc",
        "build": "npm run styles && npm run icons && npm run copycomb",
        "dev": "docsify serve docs"
    },
    "files": [
        "build/*"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ForsysInteractive/biings-ds.git"
    },
    "keywords": [
        "biings",
        "design system",
        "icons"
    ],
    "author": {
        "name": "Biings Technologies SA",
        "email": "info@biings.com",
        "url": "https://www.biings.com"
    },
    "contributors": [
        "Frédéric Andreae"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/ForsysInteractive/biings-ds/issues",
        "email": "info@biings.com"
    },
    "homepage": "https://biings.design",
    "browserslist": [
        "defaults",
        "not ie <= 8"
    ],
    "devDependencies": {
        "bulma": "^0.7.5",
        "cssnano": "^3.10.0",
        "grunt": "~0.4.1",
        "grunt-cli": "^1.2.0",
        "grunt-contrib-clean": "~0.5.0",
        "grunt-contrib-copy": "~0.5.0",
        "grunt-contrib-less": "~0.11.0",
        "grunt-contrib-requirejs": "~0.4.4",
        "grunt-svgmin": "~0.4.0",
        "grunt-svgstore": "^0.5.0",
        "node-sass": "^4.9.0",
        "postcss": "^6.0.22",
        "postcss-cli": "^5.0.0",
        "postcss-import": "^11.1.0",
        "postcss-preset-env": "^5.1.0"
    },
    "dependencies": {}
}
