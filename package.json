{
  "name": "biings-ds",
  "version": "1.3.2",
  "description": "Biings Design System",
  "main": "build/bds.css",
  "scripts": {
    "sass": "node_modules/node-sass/bin/node-sass --include-path node_modules src/styles/bulma/bulma.sass src/styles/bulma/bulma.css",
    "postcss": "postcss src/styles/bds.css --config src/styles/postcss.config.js -o build/bds.css",
    "styles": "npm run sass && npm run postcss",
    "stylesdoc": "npm run sass && postcss src/styles/bds.css --config src/styles/postcss.config.js -o docs/bds.css",
    "icons": "grunt",
    "build": "npm run styles && npm run icons"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ForsysInteractive/biings-ds.git"
  },
  "keywords": [
    "biings",
    "design system",
    "icons"
  ],
  "author": {
    "name": "Biings Technologies SA",
    "email": "info@biings.com",
    "url": "https://www.biings.com"
  },
  "contributors": [
    "Frédéric Andreae",
    "Malik Guerid"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ForsysInteractive/biings-ds/issues",
    "email": "info@biings.com"
  },
  "homepage": "https://github.com/ForsysInteractive/biings-ds#readme",
  "browserslist": [
    "defaults",
    "not ie <= 8"
  ],
  "devDependencies": {
    "bulma": "^0.7.1",
    "cssnano": "^3.10.0",
    "grunt": "~0.4.1",
    "grunt-cli": "^1.2.0",
    "grunt-contrib-clean": "~0.5.0",
    "grunt-contrib-copy": "~0.5.0",
    "grunt-contrib-less": "~0.11.0",
    "grunt-contrib-requirejs": "~0.4.4",
    "grunt-svgmin": "~0.4.0",
    "grunt-svgstore": "^0.5.0",
    "node-sass": "^4.9.0",
    "postcss": "^6.0.22",
    "postcss-cli": "^5.0.0",
    "postcss-import": "^11.1.0",
    "postcss-preset-env": "^5.1.0"
  }
}
